{
  "name": "@vaadin/generator-typescript-plugin-backbone",
  "version": "0.1.0",
  "description": "A Fusion TypeScript Generator plugin to generate basic code",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">= 16.13"
  },
  "scripts": {
    "clean:build": "rimraf *.{d.ts.map,d.ts,js.map,js}",
    "build": "concurrently npm:build:*",
    "build:transpile": "tsc --isolatedModules",
    "build:copy": "copyfiles -f src/**/*.d.ts .",
    "test": "mocha test/**/*.spec.ts --config ../../../.mocharc.cjs",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vaadin/fusion.git",
    "directory": "packages/ts/generator-typescript-plugin-backbone"
  },
  "keywords": [
    "fusion",
    "typescript",
    "generator"
  ],
  "author": "Vaadin Ltd.",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/vaadin/fusion/issues"
  },
  "homepage": "https://vaadin.com/fusion",
  "peerDependencies": {
    "@vaadin/generator-typescript-core": "^0.1.0"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "openapi-types": "^9.3.0",
    "pino": "^7.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.5",
    "@vaadin/generator-typescript-core": "^0.1.0",
    "chai": "^4.3.4",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.1",
    "mocha": "^9.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "type-fest": "^2.5.1"
  }
}
